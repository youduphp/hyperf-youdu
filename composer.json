{
    "name": "youduphp/laravel-youdu",
    "description": "The youdu component for laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "youdu"
    ],
    "homepage": "https://github.com/youduphp/laravel-youdu",
    "license": "MIT",
    "authors": [{
        "name": "huangdijia",
        "email": "huangdijia@gmail.com"
    }],
    "require": {
        "php": ">=8.0",
        "hyperf/config": "~3.0.0",
        "hyperf/di": "~3.0.0",
        "hyperf/utils": "~3.0.0",
        "youduphp/youdu": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0",
        "pestphp/pest": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "YouduPhp\\HyperfYoudu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YouduPhp\\HyperfYoudu\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "pest",
        "cs-fix": "php-cs-fixer fix $1",
        "analyse": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon ./src ./config"
    },
    "extra": {
        "laravel": {
            "providers": [
                "YouduPhp\\HyperfYoudu\\ConfigProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
